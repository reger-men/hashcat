  u32 salt_buf0[4];

  salt_buf0[0] = hc_swap32_S (salt_bufs[salt_pos].salt_buf[0]);
  salt_buf0[1] = hc_swap32_S (salt_bufs[salt_pos].salt_buf[1]);
